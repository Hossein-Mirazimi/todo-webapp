{
  "name": "@app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "esno server",
    "dev:spa": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vue-tsc -b && vite build --outDir dist/client --ssrManifest",
    "build:server": "vue-tsc -b && vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "cross-env NODE_ENV=production esno server",
    "benchmark": "esno ./tools/benchmark.ts"
  },
  "dependencies": {
    "esno": "^4.8.0"
  },
  "devDependencies": {
    "axios": "^1.7.9",
    "k6": "^0.0.0"
  }
}
