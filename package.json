{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "esno server",
    "dev:spa": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vue-tsc -b && vite build --outDir dist/client --ssrManifest",
    "build:server": "vue-tsc -b && vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "cross-env NODE_ENV=production esno server",
    "benchmark": "esno ./tools/benchmark.ts"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.5.13",
    "@vueuse/head": "^2.0.0",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "esno": "^4.8.0",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "sirv": "^3.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "axios": "^1.7.9",
    "k6": "^0.0.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-pages": "^0.32.4",
    "vue-tsc": "^2.1.10"
  }
}
